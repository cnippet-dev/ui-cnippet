{
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "prettier": "^3.7.4",
    "turbo": "^2.7.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "name": "ui-cnippet",
  "packageManager": "bun@1.3.6",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev",
    "format": "biome format --write .",
    "format:all": "biome check --write --unsafe --no-errors-on-unmatched",
    "lint": "turbo run lint --"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
