      "content": "import { Button } from \"@/components/ui/button\";\r\nimport {\r\n  Dialog,\r\n  DialogClose,\r\n  DialogDescription,\r\n  DialogFooter,\r\n  DialogHeader,\r\n  DialogPanel,\r\n  DialogPopup,\r\n  DialogTitle,\r\n  DialogTrigger,\r\n} from \"@/components/ui/dialog\";\r\nimport { Field, FieldLabel } from \"@/components/ui/field\";\r\nimport { Input } from \"@/components/ui/input\";\r\n\r\nexport default function Particle() {\r\n  return (\r\n    <Dialog>\r\n      <DialogTrigger render={<Button variant=\"outline\" />}>\r\n        Open parent\r\n      </DialogTrigger>\r\n      <DialogPopup showCloseButton={false}>\r\n        <DialogHeader>\r\n          <DialogTitle>Manage team member</DialogTitle>\r\n          <DialogDescription>\r\n            View and manage a user in your team.\r\n          </DialogDescription>\r\n        </DialogHeader>\r\n        <DialogPanel className=\"grid gap-4\">\r\n          <div className=\"grid gap-1\">\r\n            <p className=\"text-muted-foreground text-sm\">Name</p>\r\n            <p className=\"font-medium text-sm\">Bora Baloglu</p>\r\n          </div>\r\n          <div className=\"grid gap-1\">\r\n            <p className=\"text-muted-foreground text-sm\">Email</p>\r\n            <p className=\"font-medium text-sm\">bora@example.com</p>\r\n          </div>\r\n        </DialogPanel>\r\n        <DialogFooter>\r\n          <Dialog>\r\n            <DialogTrigger render={<Button variant=\"outline\" />}>\r\n              Edit details\r\n            </DialogTrigger>\r\n            <DialogPopup showCloseButton={false}>\r\n              <DialogHeader>\r\n                <DialogTitle>Edit details</DialogTitle>\r\n                <DialogDescription>\r\n                  Make changes to the member&apos;s information.\r\n                </DialogDescription>\r\n              </DialogHeader>\r\n              <DialogPanel className=\"grid gap-4\">\r\n                <Field>\r\n                  <FieldLabel>Name</FieldLabel>\r\n                  <Input defaultValue=\"Bora Baloglu\" type=\"text\" />\r\n                </Field>\r\n                <Field>\r\n                  <FieldLabel>Email</FieldLabel>\r\n                  <Input defaultValue=\"bora@example.com\" type=\"text\" />\r\n                </Field>\r\n              </DialogPanel>\r\n              <DialogFooter>\r\n                <DialogClose render={<Button variant=\"ghost\" />}>\r\n                  Cancel\r\n                </DialogClose>\r\n                <Button type=\"submit\">Save changes</Button>\r\n              </DialogFooter>\r\n            </DialogPopup>\r\n          </Dialog>\r\n        </DialogFooter>\r\n      </DialogPopup>\r\n    </Dialog>\r\n  );\r\n}\r\n",
