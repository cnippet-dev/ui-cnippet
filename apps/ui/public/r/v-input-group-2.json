      "content": "import {\r\n  InputGroup,\r\n  InputGroupAddon,\r\n  InputGroupInput,\r\n  InputGroupText,\r\n} from \"@/components/ui/input-group\";\r\n\r\nexport default function Particle() {\r\n  return (\r\n    <div className=\"space-y-4\">\r\n      <div className=\"space-y-2 [&_p]:text-xs\">\r\n        <p>With Start Text</p>\r\n        <InputGroup>\r\n          <InputGroupInput\r\n            aria-label=\"Set your URL\"\r\n            className=\"*:[input]:ps-0!\"\r\n            placeholder=\"cnippet\"\r\n            type=\"search\"\r\n          />\r\n          <InputGroupAddon>\r\n            <InputGroupText>i.cnippet.dev/</InputGroupText>\r\n          </InputGroupAddon>\r\n        </InputGroup>\r\n      </div>\r\n      <div className=\"space-y-2 [&_p]:text-xs\">\r\n        <p>With End Text</p>\r\n        <InputGroup>\r\n          <InputGroupInput\r\n            aria-label=\"Choose a username\"\r\n            placeholder=\"Choose a username\"\r\n            type=\"text\"\r\n          />\r\n          <InputGroupAddon align=\"inline-end\">\r\n            <InputGroupText>@cnipept.dev</InputGroupText>\r\n          </InputGroupAddon>\r\n        </InputGroup>\r\n      </div>\r\n      <div className=\"space-y-2 [&_p]:text-xs\">\r\n        <p>With Start and End Text</p>\r\n        <InputGroup>\r\n          <InputGroupInput\r\n            aria-label=\"Enter your domain\"\r\n            className=\"*:[input]:px-0!\"\r\n            placeholder=\"cnippet\"\r\n            type=\"text\"\r\n          />\r\n          <InputGroupAddon>\r\n            <InputGroupText>https://</InputGroupText>\r\n          </InputGroupAddon>\r\n          <InputGroupAddon align=\"inline-end\">\r\n            <InputGroupText>.dev</InputGroupText>\r\n          </InputGroupAddon>\r\n        </InputGroup>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n",
      "type": "registry:block"
    }
  ],
